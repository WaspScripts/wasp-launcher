{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "store:default",
    "fs:default",
    "http:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "http://localhost:*"
        },
        {
          "url": "https://api.waspscripts.dev"
        },
        {
          "url": "https://db.waspscripts.dev"
        }
      ]
    }
  ]
}